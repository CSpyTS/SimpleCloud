<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cspy.online.dao.StorageMapper">

    <select id="getStorageByUid" resultType="java.lang.Long">
        SELECT SUM(`change`) FROM storage_table WHERE  `uid` = #{uid}
    </select>


</mapper>